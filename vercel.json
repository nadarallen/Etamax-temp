{
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*).(jpg|jpeg|png|webp|svg|mp3)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).(css|js)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400"
                }
            ]
        }
    ]
}